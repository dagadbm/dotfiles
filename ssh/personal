Host github-personal github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/github_personal

Host vps-root
    User root
    Port 2222
    IdentityFile ~/.ssh/vps_root
    ServerAliveInterval 60
    ServerAliveCountMax 10

Host vps-openclaw
    User openclaw
    Port 2222
    IdentityFile ~/.ssh/vps_openclaw
    ServerAliveInterval 60
    ServerAliveCountMax 10
# vim: set ft=sshconfig:
